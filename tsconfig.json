{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "Node",
    "removeComments": true,
    "importHelpers": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": [
    "node_modules",
    ".serverless",
    ".webpack",
    "webpack.config.js",
    "**/*.js"
  ]
}
